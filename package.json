    "normalize-scss": "^7.0.1",
    "reflect-metadata": "^0.1.13",
    "vue": "^2.6.10",
    "vue-i18n": "^8.12.0",
    "vue-property-decorator": "^8.2.1",
    "vue-router": "^3.0.7",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
